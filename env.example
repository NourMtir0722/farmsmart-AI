# FarmSmart AI Environment Variables
# Copy this file to .env.local and fill in your API keys

# Plant.id API Configuration
# Get your API key from: https://web.plant.id/api-access-request
PLANT_ID_API_KEY=your_plant_id_api_key_here

# Google Vision API Configuration (Optional)
# Get your API key from: https://console.cloud.google.com/
GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Environment Configuration
# Set to 'production' to use real APIs instead of mock data
NODE_ENV=development

# Optional: Database Configuration (for future features)
# DATABASE_URL=your_database_connection_string

# Optional: Authentication (for future features)
# NEXTAUTH_SECRET=your_nextauth_secret
# NEXTAUTH_URL=http://localhost:3000 